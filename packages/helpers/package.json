{
  "name": "@k8o/helpers",
  "version": "1.0.0",
  "description": "Helper functions for k8o project",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*/index.ts"
  },
  "scripts": {
    "lint": "eslint --max-warnings 0 ./src",
    "lint:fix": "eslint --fix --max-warnings 0 ./src",
    "format": "prettier --write --experimental-cli src/",
    "format:check": "prettier --check --experimental-cli src/",
    "test": "vitest",
    "type-check": "tsgo --noEmit"
  },
  "dependencies": {
    "@vercel/functions": "2.2.4",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "remark": "15.0.1",
    "tailwind-merge": "3.3.1",
    "to-vfile": "8.0.0",
    "vfile-matter": "5.0.1"
  },
  "devDependencies": {
    "@types/mdast": "4.0.4"
  }
}
