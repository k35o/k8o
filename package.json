{
  "name": "k8o",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.19.0"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^22.18.0",
      "onFail": "download"
    }
  },
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check": "turbo check",
    "check:write": "turbo check:write",
    "ls-lint": "ls-lint",
    "type-check": "turbo type-check",
    "test": "TZ=UTC vitest",
    "test:ui": "TZ=UTC vitest --ui --coverage",
    "coverage": "TZ=UTC vitest run --coverage",
    "install-playwright": "playwright install --with-deps",
    "mcp-playwright": "mcp-server-playwright"
  },
  "dependencies": {
    "@k8o/helpers": "workspace:*",
    "@upstash/ratelimit": "2.0.6",
    "@upstash/redis": "1.35.4",
    "remark-frontmatter": "5.0.0",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@ls-lint/ls-lint": "2.3.1",
    "@playwright/mcp": "0.0.39",
    "@types/node": "22.18.6",
    "@typescript/native-preview": "7.0.0-dev.20250923.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-istanbul": "3.2.4",
    "@vitest/ui": "3.2.4",
    "lefthook": "1.13.4",
    "playwright": "1.55.1",
    "turbo": "2.5.8",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  }
}
