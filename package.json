{
  "name": "k8o",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.21.1"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^22.18.0",
      "onFail": "download"
    }
  },
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check": "turbo check",
    "check:write": "turbo check:write",
    "ls-lint": "ls-lint",
    "type-check": "turbo type-check",
    "test": "TZ=UTC vitest",
    "test:ui": "TZ=UTC vitest --ui --coverage",
    "coverage": "TZ=UTC vitest run --coverage",
    "install-playwright": "playwright install --with-deps",
    "mcp-playwright": "mcp-server-playwright",
    "mcp-chrome": "chrome-devtools-mcp"
  },
  "dependencies": {
    "@k8o/helpers": "workspace:*",
    "@upstash/ratelimit": "2.0.6",
    "@upstash/redis": "1.35.6",
    "remark-frontmatter": "5.0.0",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@ls-lint/ls-lint": "2.3.1",
    "@playwright/mcp": "0.0.45",
    "@types/node": "22.18.13",
    "@typescript/native-preview": "7.0.0-dev.20251107.1",
    "@vitest/coverage-istanbul": "4.0.6",
    "@vitest/ui": "4.0.6",
    "chrome-devtools-mcp": "0.9.0",
    "lefthook": "2.0.2",
    "playwright": "1.56.1",
    "turbo": "2.6.0",
    "typescript": "5.9.3",
    "vitest": "4.0.6"
  }
}
