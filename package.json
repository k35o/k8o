    "rehype-katex": "7.0.0",
    "rehype-pretty-code": "0.13.2",
    "remark-math": "6.0.0",
    "shiki": "1.14.1",
    "tailwind-merge": "2.5.2",
    "tailwindcss": "3.4.10",
    "zod": "3.23.8"
