    "clsx": "2.1.1",
    "drizzle-orm": "0.33.0",
    "framer-motion": "11.5.6",
    "lucide-react": "0.446.0",
    "next": "14.2.13",
    "next-themes": "0.3.0",
    "postcss": "8.4.47",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "rehype-katex": "7.0.1",
    "rehype-pretty-code": "0.14.0",
    "remark-math": "6.0.0",
    "shiki": "1.18.0",
    "tailwind-merge": "2.5.2",
    "tailwindcss": "3.4.12",
    "zod": "3.23.8"
