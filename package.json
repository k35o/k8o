{
  "name": "k8o",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.21.1"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^22.18.0",
      "onFail": "download"
    }
  },
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check": "biome check",
    "check:write": "biome check --write --unsafe",
    "ls-lint": "ls-lint",
    "type-check": "turbo type-check",
    "test": "TZ=UTC vitest",
    "test:ui": "TZ=UTC vitest --ui --coverage",
    "coverage": "TZ=UTC vitest run --coverage",
    "install-playwright": "playwright install --with-deps",
    "mcp-chrome": "chrome-devtools-mcp",
    "mcp-next": "next-devtools-mcp"
  },
  "dependencies": {
    "@repo/helpers": "workspace:*",
    "@upstash/ratelimit": "2.0.7",
    "@upstash/redis": "1.35.7",
    "remark-frontmatter": "5.0.0",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@ls-lint/ls-lint": "2.3.1",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "22.19.0",
    "@typescript/native-preview": "7.0.0-dev.20251206.1",
    "@vitest/coverage-istanbul": "4.0.15",
    "@vitest/ui": "4.0.15",
    "chrome-devtools-mcp": "0.11.0",
    "lefthook": "2.0.8",
    "next-devtools-mcp": "0.3.6",
    "playwright": "1.57.0",
    "turbo": "2.6.3",
    "typescript": "5.9.3",
    "vitest": "4.0.15"
  }
}
