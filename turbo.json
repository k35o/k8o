{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "check": {
      "dependsOn": ["^check"]
    },
    "check:write": {
      "dependsOn": ["^check:write"]
    },
    "type-check": {
      "dependsOn": ["^type-check"]
    }
  }
}
